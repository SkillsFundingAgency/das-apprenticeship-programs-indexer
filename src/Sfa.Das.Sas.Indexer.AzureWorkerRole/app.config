    <add key="LoggingRedisConnectionString" value="" />
    <add key="LoggingRedisKey" value="logstash" />
    <add key="ValidOrganisationTypes" value="Assessment Organisation|Awarding Organisation|Employer or trade body|Higher Education Institution|Other|Professional body|Sector Skills Council|Training Provider" />
